{
	"$schema": "https://unpkg.com/jsrepo@2.4.0/schemas/registry-config.json",
	"name": "yourteam/test-registry",
	"version": "0.0.1",
	"readme": "README.md",
	"dirs": [
		"./src/app/shared/components/base/*",
    "./src/app/shared/pipes"
